generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model user {
  id        String    @id @default(uuid())
  firstName String?
  lastName  String?
  picture   String
  email     String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  detateAt  DateTime?
}
